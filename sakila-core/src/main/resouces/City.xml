<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.uniyaz.tum.City">
        <table name="city" schema="sakila" catalog=""/>
        <attributes>
            <id name="cityId">
                <column name="city_id"/>
            </id>
            <basic name="city">
                <column name="city"/>
            </basic>
            <basic name="lastUpdate">
                <column name="last_update"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
