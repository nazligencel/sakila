<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.uniyaz.tum.Store">
        <table name="store" schema="sakila" catalog=""/>
        <attributes>
            <id name="storeId">
                <column name="store_id"/>
            </id>
            <basic name="lastUpdate">
                <column name="last_update"/>
            </basic>
            <many-to-one name="staffByManagerStaffId" optional="true" target-entity="com.uniyaz.tum.Staff">
                <join-column name="manager_staff_id" referenced-column-name="staff_id"/>
            </many-to-one>
            <many-to-one name="addressByAddressId" optional="true" target-entity="com.uniyaz.address.domain.Address">
                <join-column name="address_id" referenced-column-name="address_id"/>
            </many-to-one>
        </attributes>
    </entity>
</entity-mappings>
