<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
        version="2.0">

    <entity class="com.uniyaz.tum.CustomerList">
        <table name="customer_list" schema="sakila" catalog=""/>
