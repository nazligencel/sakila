            <basic name="id">
                <column name="ID"/>
            </basic>
            <basic name="name">
                <column name="name"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
            <basic name="zipCode">
                <column name="zip code"/>
            </basic>
            <basic name="phone">
                <column name="phone"/>
            </basic>
            <basic name="city">
                <column name="city"/>
            </basic>
            <basic name="country">
                <column name="country"/>
            </basic>
            <basic name="notes">
                <column name="notes"/>
            </basic>
            <basic name="sid">
                <column name="SID"/>
            </basic>
